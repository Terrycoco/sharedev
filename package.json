{
  "name": "sharedev",
  "version": "1.0.0",
  "private": true,
  "description": "sharewalks dev",
  "main": "index.js",
  "config": {
    "prodbundle": "../sharewalks/public/"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "copySw": "cp ./public/serviceworkers/sw.js ./public/sw.js",
    "predev": "npm run copySw",
    "dev": "NODE_ENV=development webpack-dev-server --env.dev --hot --inline --progress --colors",
    "build": "export NODE_ENV=production&&webpack --config ./webpack.config --progress --colors --env.prod",
    "lint": "eslint ./client ./webpack.config.js -f table || true",
    "prebuild": "find ./public -maxdepth 1 -type f -delete && npm run copySw",
    "postbuild": "npm run toProd",
    "toProd": "rm -rf $npm_package_config_prodbundle && mkdir $npm_package_config_prodbundle",
    "posttoProd": "cp -a ./public/. $npm_package_config_prodbundle"
  },
  "author": "Terry Marr",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "autoprefixer": "^6.5.3",
    "axios": "^0.15.3",
